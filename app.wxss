.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.arrow{
  position: absolute;
  right: 60rpx;
  top: 0;
  line-height: 87rpx;
  font-size: 38rpx;
}
.pd30{
  padding: 0 30rpx;
}

.noborder{
  border-bottom: transparent !important
}

.active_btn{
  background: #317df3;
  color: #ffffff
}
.taobao_icon{
  color: #ff681d;
  margin-right: 20rpx;
}
.tianmao_icon{
  color: #ff0036;
  margin-right: 20rpx;
}
.styleFontSize{
  font-size: 32rpx
}
.fix-iphonex-footer{
  bottom: 68rpx !important
}
.fix-iphonex-footer::after {
  content: ' ';
  position: fixed;
  bottom: 0!important;
  height: 68rpx!important;
  width: 100%;
  background: #ffffff;
  z-index: 999;
}
.falsePages{
  font-size: 0;
  position: relative;
}
.falsePic{
  width: 100%;
}
.weui_dialog_confirm{
  display: flex;  
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;  
  justify-content: center;  
  align-items: center;  
  position: fixed;  
  z-index: 999; 
  background: rgba(0, 0, 0, 0.8);  
}
.weui_mask{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 1
}
.weui_dialog{
  width: 75%;  
  background: #ffffff;
  border-radius: 20rpx; 
  position: relative;
  z-index: 2;
  text-align: center
}
.weui_dialog_hd{ 
  font-size: 32rpx;
  padding: 15rpx 0;
}
.weui_dialog_bd{
  padding:0 20rpx; 
  font-size: 28rpx;
}
.first_title{
  line-height: 80rpx;
}

.second_title{
  font-size: 26rpx;
  color: #999;
  padding-bottom: 20rpx;
}
.weui_dialog_ft{
  text-align: center;
  border-top: 1px solid #f6f6f6;
}
.weui_dialog_ft button{
  background: transparent;
  font-size: 32rpx;
  color: #317EF3
}
.tx{
  overflow: hidden;
  display: block;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.ranktx{
  width:104rpx;
  height:104rpx;
  text-align:left;
  margin-top:13rpx;
  margin-right:20rpx;
}

.zuihou{
line-height: 60rpx; text-align: center;color: #B4B4B4; font-size: 28rpx;}


.nouser{
  width: 100%;
  padding-top: 100rpx;
  text-align: center
}
.norank{
  padding-top: 0;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 100%;
}
.nofont{
  height:120rpx;
  line-height:120rpx;
  font-size:15px;
  color: #d0d0d0
}
.falsePages_img{
  width: 40%;
  display:block;
  margin:0 auto;
}
.wx_dialog_container{
  display: none;
}
.wx_dialog_button__block{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: transparent;
  padding: 0
}
.wx_dialog_buttons{
  display: none;
}
.wx_dialog_buttons::after{
  border: none
}
.page_box{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  /* background: #36c3e5 */
}
.loading_img{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 280rpx;
  height: 280rpx;
}